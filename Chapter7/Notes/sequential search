public static int sequentialSearch (int [] array, int value) // 87 75 98 100 82
{
    for (int i = 0; i < array.length; i++)
    {
    if (array [i] == value){        // return the index of the wanted value
        return i;
    }
    return -1;                  // if search fails, return -1
    }            
